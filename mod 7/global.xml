<http:listener-config name="HTTP_Listener_config" doc:name="HTTP Listener config" doc:id="673c27de-2df8-4a73-aa08-b63a6613e1b9" basePath="http/">
		<http:listener-connection host="0.0.0.0" port="${http.port}" />
